# Telegram bot token
BOT_TOKEN=your_bot_token_here

# ADMIN IDS 
ADMINS=admin_id

# Debug mode (True or False)
DEBUG=False

# Optional proxy, e.g., socks5://127.0.0.1:1080
PROXY=socks5://host.docker.internal:2080

# Database configuration
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=secret
DB_NAME=soundcloud_bot

# Local directories
DOWNLOAD_DIR=downloads
LOGGING_DIR=logs

# Default language
DEFAULT_LANG=en

# Maximum concurrent downloads
MAX_CONCURRENT_DOWNLOADS=3
